<Window x:Class="Chancellery.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chancellery"
        mc:Ignorable="d"
        MinHeight="600" MinWidth="800">
    <Grid Background="#FFCBF1A7">
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="200"/>
            </Grid.ColumnDefinitions>
            <TextBlock Text="Магазин канцелярии" Width="auto" FontSize="20" Margin="299,15,107,15"/>
            <Button Grid.Column="1" Margin="25,15" Content="Назад" Click="GoBack_Click"/>
        </Grid>
        <Frame Name="MainFrame" Grid.Row="1" Background="White" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
